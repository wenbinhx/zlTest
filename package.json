{
  "name": "wisdom",
  "version": "1.1.0",
  "license": "MIT",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "author": "Liu <liuyulin_ios@foxmail.com>",
  "scripts": {
    "dev": "cross-env env_config=dev webpack-dev-server --inline --progress --host 0.0.0.0 --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "dev:debugger": "cross-env env_config=debugger webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:mock": "cross-env env_config=mock webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build": "cross-env node build/build.js",
    "build:test": "cross-env env_config=test node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint",
    "dll": "webpack --config ./build/webpack.dll.config.js"
  },
  "dependencies": {
    "@antv/f2": "^3.1.21",
    "axios": "0.17.1",
    "babel-polyfill": "^6.26.0",
    "better-scroll": "^1.12.6",
    "cube-ui": "^1.10.10",
    "fastclick": "^1.0.6",
    "countup.js": "^1.8.1",
    "js-cookie": "2.2.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "vue": "2.5.10",
    "vue-i18n": "^8.0.0",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "compileDependencies": [
    "cube-ui"
  ],
  "transformModules": {
    "cube-ui": {
      "transform": "cube-ui/lib/${member}",
      "kebabCase": true,
      "style": {
        "ignore": [
          "create-api",
          "better-scroll"
        ]
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "3.5.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "2.3.0",
    "copy-webpack-plugin": "4.2.3",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.7",
    "eslint": "4.13.1",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "4.0.1",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "friendly-errors-webpack-plugin": "1.6.1",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "2.30.1",
    "node-notifier": "5.1.2",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "ora": "1.3.0",
    "portfinder": "1.0.13",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.9",
    "postcss-pxtorem": "^4.0.1",
    "postcss-url": "7.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "semver": "5.4.1",
    "shelljs": "0.7.8",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "svg-sprite-loader": "3.5.2",
    "uglifyjs-webpack-plugin": "1.1.3",
    "url-loader": "0.6.2",
    "vue-loader": "13.7.2",
    "vue-style-loader": "3.0.3",
    "vue-template-compiler": "2.5.10",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.1",
    "webpack-dev-server": "2.9.7",
    "webpack-merge": "4.1.1",
    "webpack-post-compile-plugin": "^0.4.1",
    "webpack-transform-modules-plugin": "^0.3.5"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
